<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.devcamp.belajaryukak">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="id.devcamp.belajaryukak.AboutActivity"></activity>
        <activity android:name="id.devcamp.belajaryukak.Quis.ResultActivity" />
        <activity android:name="id.devcamp.belajaryukak.Quis.QuisActivity" />
        <activity android:name="id.devcamp.belajaryukak.Mapel.MapelIndoActivity" />
        <activity android:name="id.devcamp.belajaryukak.Mapel.MapelPknActivity" />
        <activity android:name="id.devcamp.belajaryukak.Mapel.MapelSenBudActivity" />
        <activity android:name="id.devcamp.belajaryukak.Mapel.MapelPenjasActivity" />
        <activity android:name="id.devcamp.belajaryukak.Mapel.DetailMapelActivity" />
        <activity
            android:name="id.devcamp.belajaryukak.HomeActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="id.devcamp.belajaryukak.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>